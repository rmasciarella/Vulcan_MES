{
  "name": "@vulcan/backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "fastapi dev app/main.py --reload",
    "start": "fastapi run app/main.py",
    "test": "./scripts/test.sh",
    "test:e2e": "pytest -c pytest-e2e.ini",
    "format": "./scripts/format.sh",
    "lint": "./scripts/lint.sh",
    "db:migrate": "alembic upgrade head",
    "db:generate": "alembic revision --autogenerate -m",
    "clean": "rm -rf .pytest_cache .mypy_cache .ruff_cache __pycache__ htmlcov .coverage"
  },
  "dependencies": {},
  "devDependencies": {}
}